
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model book {

  id           String @id @default(uuid())
  title        String
  description  String
  bar_code     String @unique 

  @@map("books")
}

model RentedBook {

  id           String @id @default(uuid())
  title        String
  description  String
  bar_code     String @unique 
  rented       String
  date         String
  UserName     String
  cpf          String @unique
  
  @@map("rentedBooks")
}
